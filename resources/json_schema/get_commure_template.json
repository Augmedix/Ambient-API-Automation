{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": { "type": "string" },
            "name": { "type": "string" },
            "content": { "type": "string" },
            "createdDate": { "type": "string", "format": "date-time" },
            "modifiedDate": { "type": "string", "format": "date-time" }
        },
        "required": ["id", "name", "content", "createdDate", "modifiedDate"]
    }
}